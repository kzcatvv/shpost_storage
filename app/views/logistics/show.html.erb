<%- model_class = Logistic -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @logistic.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:print_format) %>:</strong></dt>
  <dd><%= @logistic.print_format %></dd>
  <dt><strong><%= model_class.human_attribute_name(:is_getnum) %>:</strong></dt>
  <dd><%= @logistic.is_getnum %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contact) %>:</strong></dt>
  <dd><%= @logistic.contact %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @logistic.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contact) %>:</strong></dt>
  <dd><%= @logistic.contact %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contact_phone) %>:</strong></dt>
  <dd><%= @logistic.contact_phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:post) %>:</strong></dt>
  <dd><%= @logistic.post %></dd>
  <dt><strong><%= model_class.human_attribute_name(:is_default) %>:</strong></dt>
  <dd><%= @logistic.is_default %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              logistics_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_logistic_path(@logistic), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              logistic_path(@logistic),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => '确定删除?')) },
              :class => 'btn btn-danger' %>
