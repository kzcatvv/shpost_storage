<%- model_class = Keyclientorder -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:keyclient_name) %>:</strong></dt>
  <dd><%= @keyclientorder.keyclient_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:keyclient_addr) %>:</strong></dt>
  <dd><%= @keyclientorder.keyclient_addr %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contact_person) %>:</strong></dt>
  <dd><%= @keyclientorder.contact_person %></dd>
  <dt><strong><%= model_class.human_attribute_name(:phone) %>:</strong></dt>
  <dd><%= @keyclientorder.phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:desc) %>:</strong></dt>
  <dd><%= @keyclientorder.desc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:batch_id) %>:</strong></dt>
  <dd><%= @keyclientorder.batch_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              keyclientorders_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_keyclientorder_path(@keyclientorder), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              keyclientorder_path(@keyclientorder),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
