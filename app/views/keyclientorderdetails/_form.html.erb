<%= form_for ([ @keyclientorder, @keyclientorderdetail]), :html => { :class => 'form-horizontal' } do |f| %>

  <div class="form-group">
    <%= f.label :specification_id, :class => 'control-label' %>
    <div class="controls">
      <%= select(:goodstype_id, nil , Goodstype.all.map{|u| [u.name,u.id]},
                 {:prompt => "选择"}, data: { remote: true,
                  :url => "/thirdpartcodes/select_commodities" 
                    }) %>
      <span>
        <%= render 'thirdpartcodes/commodities' %>
      </span>
    </div>

  </div>
  <div class="form-group">
    <%= f.label :desc, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :desc, :class => 'form-control' %>
    </div>
  </div>

  
  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
            keyclientorder_keyclientorderdetails_path(@keyclientorder), :class => 'btn btn-default' %>
 
<% end %>
